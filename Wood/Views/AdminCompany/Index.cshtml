
@{
    Layout = null;
}
@model WoodModel.CompanyModel
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>公司信息编辑</title>
    <link href="../../Content/styles/admin/style.css" rel="stylesheet" type="text/css" />
    <link href="~/Scripts/Admin/AdminPqGrid/pqgrid.min.css" rel="stylesheet" />

    <script src="~/Scripts/Admin/jquery-1.8.2.min.js"></script>
    <script src="~/Scripts/Admin/jquery.easyui.min.js"></script>
    <script src="~/Scripts/Admin/AdminPqGrid/pqgrid.min.js"></script>
    <script src="~/Scripts/Admin/FunctionJS.js"></script>
    <script src="~/Scripts/AdminCompnay/index.js"></script>
</head>
<body>
    <div class="btnbartitle">
        <div>
            管理信息 - <span id="OrganizationName">公司信息</span>
        </div>
    </div>

    <div class="tools_bar">
            <a title="刷新当前页面" onclick="Replace();" class="tools_btn">
                <span><b style="background: url('/Content/images/admin/16/arrow_refresh.png') 50% 4px no-repeat;">刷新</b></span>
            </a><div class="tools_separator"></div>
            @*<a title="" onclick="add();" class="tools_btn">
                <span><b style="background: url('/Content/images/admin/16/application_add.png') 50% 4px no-repeat;">新增</b></span>
            </a>*@
            <a title="" onclick="edit();" class="tools_btn"><span class=""><b style="background: url('/Content/images/admin/16/application_edit.png') 50% 4px no-repeat;">编辑</b></span></a>
            @*<a title="" onclick="Delete();" class="tools_btn"><span><b style="background: url('/Content/images/admin/16/application_delete.png') 50% 4px no-repeat;">删除</b></span></a>*@
            <div class="tools_separator"></div>
            <a title="关闭当前窗口" onclick="ThisCloseTab();" class="tools_btn">
                <span class=""><b style="background: url('/Content/images/admin/16/back.png') 50% 4px no-repeat;">离开</b></span>
            </a>
    </div>
    <table id="tb_basic" border="0" cellpadding="0" cellspacing="0" class="frm tabPanel" style="margin-top: 1px; border-top-width: 1px; border-top-style: solid; border-top-color: rgb(204, 204, 204); display: table;">
        <tbody>
            <tr>
                <th class="highlight">
                    编号：
                </th>
                <td>
                    <span id="Code">admin</span>
                </td>
                
            </tr>
            <tr>
                <th class="highlight">
                    姓名：
                </th>
                <td>
                    <span id="RealName">佘赐雄</span>
                </td>
               
            </tr>
            <tr>
                <th class="highlight">
                    性别：
                </th>
                <td>
                    <span id="Gender">男</span>
                </td>
                
            </tr>
            <tr>
                <th class="highlight">
                    出生日期：
                </th>
                <td>
                    <span id="Birthday">2013/10/28 18:49:23</span>
                </td>
              
            </tr>
            <tr>
                <th class="highlight">
                    职位：
                </th>
                <td>
                    <span id="DutyId">工程师</span>
                </td>
               
            </tr>
            <tr>
                <th class="highlight">
                    职称：
                </th>
                <td>
                    <span id="TitleId">软件工程师</span>
                </td>

                
            </tr>
            <tr>

                <th class="highlight">
                    公司：
                </th>
                <td>
                    <span id="CompanyId">上海分公司</span>
                </td>
              
            </tr>
            <tr>
                <th class="highlight">
                    部门名称：
                </th>
                <td>
                    <span id="DepartmentId">IT信息部</span>
                </td>
                
            </tr>
            <tr>
                <th class="highlight">
                    有效性：
                </th>
                <td colspan="3">
                    <span id="Enabled">是</span>
                </td>
            </tr>
            <tr>
                <th class="highlight">
                    说明：
                </th>
                <td colspan="3">
                    <span id="Description">测试</span>
                </td>
            </tr>
        </tbody>
    </table>
    @*<div class="formbody">

        <div class="formtitle"><span>基本信息</span></div>

        <ul class="forminfo">
            <li><label>公司名称</label>
                @Html.TextBoxFor(Model => Model.CompanyName, new { @class = "dfinput" })
                <i>公司名称不能超过30个字符</i></li>
            <li><label>公司简介</label>
                @Html.TextAreaFor(Model => Model.CompanySummary, 5, 50, new { @class = "dfinput" })
               </li>
            <li><label>详细介绍</label>
                @Html.TextAreaFor(Model => Model.CompanyDes, 10, 50, new { @class = "dfinput" })
                </li>
            <li><label>公司地址</label>
                @Html.TextBoxFor(Model => Model.CompanyAddress, new { @class = "dfinput" })
                </li>
            <li><label>公司电话</label>
                @Html.TextBoxFor(Model => Model.CompanyTelephone, new { @class = "dfinput" })
                </li>
            <li><label>公司邮箱</label>
                @Html.TextBoxFor(Model => Model.CompanyEmail, new { @class = "dfinput" })
                </li>
            <li><label>&nbsp;</label>
                <input name="" type="button" class="btn" value="确认保存" onclick="SaveCompany()" /></li>
        </ul>
    </div>*@
</body>
</html>
